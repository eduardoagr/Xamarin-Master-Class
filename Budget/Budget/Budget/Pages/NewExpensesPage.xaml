<?xml version="1.0" encoding="utf-8" ?>
<rg:PopupPage
    x:Class="Budget.Pages.NewExpensesPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:community="http://xamarin.com/schemas/2020/toolkit"
    xmlns:icon="clr-namespace:DuoNotes.Fonts"
    xmlns:rg="http://rotorgames.com"
    xmlns:vm="clr-namespace:Budget.ViewModels"
    AndroidTalkbackAccessibilityWorkaround="True">

    <rg:PopupPage.BindingContext>
        <vm:NewExpensePageModel />
    </rg:PopupPage.BindingContext>

    <rg:PopupPage.Animation>
        <rg:ScaleAnimation
            DurationIn="700"
            EasingIn="BounceOut"
            PositionIn="Bottom"
            PositionOut="Center"
            ScaleIn="1"
            ScaleOut="0.7" />
    </rg:PopupPage.Animation>

    <StackLayout VerticalOptions="Center" 
                 HorizontalOptions="Center">
        <Label Text="vfdv[f"/>

    </StackLayout>
</rg:PopupPage>